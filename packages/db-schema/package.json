{
  "name": "@treksistem/db-schema",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate:local": "wrangler d1 migrations apply TREKSISTEM_DB --local",
    "db:migrate:remote": "wrangler d1 migrations apply TREKSISTEM_DB",
    "clean": "rm -rf dist drizzle"
  },
  "dependencies": {
    "drizzle-orm": "^0.30.10",
    "@treksistem/shared-types": "workspace:*"
  },
  "devDependencies": {
    "@treksistem/eslint-config-custom": "workspace:*",
    "drizzle-kit": "^0.21.1",
    "wrangler": "^3.57.1",
    "typescript": "^5.4.5"
  }
} 